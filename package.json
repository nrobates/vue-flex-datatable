{
  "name": "vue-flex-datatable",
  "version": "0.0.9",
  "description": "A flexible use-case datatable for Vue.js 2.x",
  "author": "Nicholas Bates <info@nrobates.com>",
  "private": false,
  "scripts": {
    "start": "webpack-dev-server --config docs/webpack.config.js",
    "demo": "NODE_ENV=production webpack --config docs/webpack.config.js",
    "build": "rm -rf dist && NODE_ENV=production webpack",
    "lint": "eslint src test --ext .js,.vue --fix; exit 0",
    "prepublish": "npm run lint; npm run test; npm run build",
    "test": "ava --verbose",
    "test-watch": "ava --verbose --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nrobates/vue-flex-datatable.git"
  },
  "keywords": [
    "nrobates"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrobates/vue-flex-datatable/issues"
  },
  "homepage": "https://nrobates.github.io/vue-flex-datatable",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "peerDependencies": {
    "moment": "^2.18.1",
    "vue": "^2.3.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "lodash": "^4.17.4",
    "vue-animate": "^0.0.4"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "ava-describe": "^1.0.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "browser-env": "^3.2.0",
    "css-loader": "^0.28.1",
    "diffable-html": "^2.1.0",
    "eslint": "^4.0.0",
    "eslint-config-spatie": "^2.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^2.1.0",
    "jest": "^19.0.0",
    "jest-serializer-html": "^4.0.0",
    "jest-vue-preprocessor": "^0.2.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "p-immediate": "^2.1.0",
    "sass-loader": "^6.0.6",
    "simulant": "^0.2.2",
    "vue": "^2.3.0",
    "vue-loader": "^12.0.3",
    "vue-node": "^1.1.1",
    "vue-template-compiler": "^2.3.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "testRegex": "test.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "snapshotSerializers": [
      "<rootDir>/tests/html-serializer"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "ava": {
    "require": [
      "babel-core/register",
      "./test/helpers/setup.js"
    ],
    "files": [
      "**/*.spec.js",
      "!**/setup.js"
    ],
    "babel": "inherit",
    "failFast": true
  }
}
